        <TextBlock x:Name="textBlock2" HorizontalAlignment="Left" Margin="0,181,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="330" FontSize="21.333" Height="179" TextAlignment="Center">
        	<Run/>
        	<LineBreak/>
        	<Run/>
        	<LineBreak/>
        	<Run Text="**Go to Orders View**"/>
        </TextBlock>
        <TextBlock x:Name="textBlock2_Copy" Margin="335,181,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="21.333" Height="179" TextAlignment="Center" LineHeight="10.667">
        	<Run/>
        	<LineBreak/>
        	<Run/>
        	<LineBreak/>
        	<Run Text="**Submit Order**"/>
        </TextBlock>

    </Grid>
</Page>
